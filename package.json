{
  "name": "nodejs-starter",
  "version": "0.1.0",
  "private": false,
  "description": "Node.js starter template",
  "keywords": [
    "javascript",
    "nodejs",
    "starter",
    "express",
    "knexjs",
    "postgresql"
  ],
  "author": {
    "name": "cednore",
    "email": "cednore@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cednore/nodejs-starter.git"
  },
  "homepage": "https://github.com/cednore/nodejs-starter",
  "bugs": {
    "url": "https://github.com/cednore/nodejs-starter/issues"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm install --production",
    "start": "node .",
    "dev": "NODE_ENV=development nodemon -r dotenv/config .",
    "test": "NODE_ENV=testing mocha",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "knex": "node -r dotenv/config node_modules/.bin/knex"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "knex": "^0.95.14",
    "pg": "^8.7.1",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "should": "^13.2.3",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6"
  }
